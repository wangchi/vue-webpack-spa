{
  "name": "vue-webpack-spa",
  "version": "0.0.1",
  "description": "A single page application using Vue, Webpack, ES6, Stylus and so on.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./server.js",
    "dev": "webpack-dev-server --config ./webpack.dev.config.js --inline --hot --progress --colors --display-error-details",
    "build": "webpack --config ./webpack.prod.config.js --process ---colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangchi/vue-webpack-spa.git"
  },
  "keywords": [
    "Vue",
    "Webpack",
    "SPA"
  ],
  "author": "Wang Chi <hiwangchi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wangchi/vue-webpack-spa/issues"
  },
  "homepage": "https://github.com/wangchi/vue-webpack-spa#readme",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "style-loader": "^0.13.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue": "^2.2.2",
    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.1.4",
    "vue-router": "^2.3.0",
    "vue-style-loader": "^2.0.3",
    "vue-template-compiler": "^2.2.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
